<div id="artist_list">
  <table>
    <h2>List of Artists</h2>
    <tr>
      <td class='category'>Signin Name</td>
      <td class='category'>First Name</td>
      <td class='category'>Last Name</td>
      <td class='category'>Created At</td>
    </tr>

    <% artists.each do |artist| %>
      <tr>
        <td><%= link_to artist.signin_name, onboarding_images_new_path(artist.signin_name) %></td>
        <td><%= link_to artist.first_name, onboarding_images_new_path(artist.signin_name) %></td>
        <td><%= link_to artist.last_name, onboarding_images_new_path(artist.signin_name) %></td>
        <td><%= link_to artist.date_created, onboarding_images_new_path(artist.signin_name) %></td>
      </tr>
    <% end %>
  <table>

</div>